{
  "manifest_version": 3,
  "name": "My Extension",
  "version": "1.0.0",
  "description": "Description of my extension",
  "permissions": [
    "activeTab"
  ],
  "background": {
    "service_worker": "background.js"
  },
  "action": {
    "default_popup": "popup.html"
  },
  "content_scripts": [
    {
      "matches": [
        "https://app.jobsoid.com/*"
      ]
    }
  ]
}